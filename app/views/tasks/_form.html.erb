<div class="w-50">
  <%= simple_form_for @task do |f| %>
    <%= f.input :title %>
    <%#= f.input :due_date, html5: true %>
    <%= f.input :description %>
    <%#= f.association :creator %>
    <%= f.association :assignees, label_method: :email, value_method: :id, as: :check_boxes, item_wrapper_tag: :div, item_wrapper_class: "" %>
    <%= f.button :submit, class: "btn btn-secondary my-3" %>
  <% end %>
</div>
